--- a/drivers/staging/mt7621-dts/Kconfig
+++ b/drivers/staging/mt7621-dts/Kconfig
@@ -9,3 +9,8 @@ config DTB_GNUBEE2
 	depends on SOC_MT7621 && DTB_RT_NONE
 	select BUILTIN_DTB
 
+config DTB_XERONT
+	bool "Xeront Router"
+	depends on SOC_MT7621 && DTB_RT_NONE
+	select BUILTIN_DTB
+
--- a/drivers/staging/mt7621-dts/Makefile
+++ b/drivers/staging/mt7621-dts/Makefile
@@ -1,5 +1,6 @@
 # SPDX-License-Identifier: GPL-2.0
 dtb-$(CONFIG_DTB_GNUBEE1)      += gbpc1.dtb
 dtb-$(CONFIG_DTB_GNUBEE2)      += gbpc2.dtb
+dtb-$(CONFIG_DTB_XERONT)       += mt7621_xeront.dtb
 
 obj-y				+= $(patsubst %.dtb, %.dtb.o, $(dtb-y))
--- /dev/null
+++ b/drivers/staging/mt7621-dts/mt7621_xeront.dts
@@ -0,0 +1,192 @@
+// SPDX-License-Identifier: GPL-2.0-or-later OR MIT
+/*
+ * Copyright (C) 2021 - 2022 Arınç ÜNAL <arinc.unal@arinc9.com>
+ */
+
+/dts-v1/;
+
+#include "mt7621.dtsi"
+
+#include <dt-bindings/input/input.h>
+
+/ {
+	compatible = "xeront", "mediatek,mt7621-soc";
+	model = "Xeront redacted (16M flash)";
+
+	aliases {
+		led-boot = &led2_green;
+		led-failsafe = &led1_red;
+		led-running = &led2_green;
+		led-upgrade = &led1_red;
+	};
+
+	chosen {
+		bootargs = "console=ttyS0,57600";
+	};
+};
+
+&ethernet {
+
+//	gmac1: mac@1 {
+//		status = "okay";
+//
+//		fixed-link {
+//			speed = <1000>;
+//			full-duplex;
+//			pause;
+//		};
+//	};
+
+	mdio-bus {
+//		ethernet-phy@5 {
+//			status = "okay";
+//		};
+
+		switch0: switch0@0 {
+			ports {
+				port@0 {
+					status = "okay";
+					label = "sw0p0";
+				};
+
+				port@1 {
+					status = "okay";
+					label = "sw0p1";
+				};
+
+				port@2 {
+					status = "okay";
+					label = "sw0p2";
+				};
+
+				port@3 {
+					status = "okay";
+					label = "sw0p3";
+				};
+
+				port@4 {
+					status = "okay";
+					label = "sw0p4";
+				};
+
+				sw0_p5: port@5 {
+					reg = <5>;
+					label = "extsw";
+					phy-mode = "rgmii";
+
+					fixed-link {
+						speed = <1000>;
+						full-duplex;
+						pause;
+					};
+				};
+			};
+		};
+
+		switch1 {
+			compatible = "realtek,rtl8367s";
+			reset-gpios = <&gpio 9 GPIO_ACTIVE_LOW>;
+			reg = <29>;
+
+			ports {
+				#address-cells = <1>;
+				#size-cells = <0>;
+
+				port@0 {
+					reg = <0>;
+					label = "sw1p0";
+				};
+
+				port@1 {
+					reg = <1>;
+					label = "sw1p1";
+				};
+
+				port@2 {
+					reg = <2>;
+					label = "sw1p2";
+				};
+
+				port@3 {
+					reg = <3>;
+					label = "sw1p3";
+				};
+
+				port@4 {
+					reg = <4>;
+					label = "sw1p4";
+				};
+
+				port@7 {
+					reg = <7>;
+					ethernet = <&sw0_p5>;
+					phy-mode = "rgmii";
+					realtek,ext-int = <2>;
+					tx-internal-delay-ps = <2000>;
+					rx-internal-delay-ps = <0>;
+
+					fixed-link {
+						speed = <1000>;
+						full-duplex;
+						pause;
+					};
+				};
+			};
+		};
+	};
+};
+
+&spi0 {
+	status = "okay";
+
+	flash@0 {
+		compatible = "jedec,spi-nor";
+		reg = <0>;
+		spi-max-frequency = <14000000>;
+
+		partitions {
+			compatible = "fixed-partitions";
+			#address-cells = <1>;
+			#size-cells = <1>;
+
+			partition@0 {
+				label = "bootloader";
+				reg = <0x0 0x30000>;
+				read-only;
+			};
+
+			partition@30000 {
+				label = "config";
+				reg = <0x30000 0x10000>;
+				read-only;
+			};
+
+			factory: partition@40000 {
+				label = "factory";
+				reg = <0x40000 0x10000>;
+				read-only;
+			};
+
+			firmware: partition@50000 {
+				compatible = "denx,uimage";
+				label = "firmware";
+				reg = <0x50000 0xfb0000>;
+			};
+		};
+	};
+};
+
+/* &gmac0 {
+	nvmem-cells = <&macaddr_factory_e000>;
+	nvmem-cell-names = "mac-address";
+}; */
+
+&factory {
+	compatible = "nvmem-cells";
+	#address-cells = <1>;
+	#size-cells = <1>;
+
+	macaddr_factory_e000: macaddr@e000 {
+		reg = <0xe000 0x6>;
+	};
+};
